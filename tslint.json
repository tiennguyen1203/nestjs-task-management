{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommend"
  ],
  "jsRule": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "ruleDirectory": []
}